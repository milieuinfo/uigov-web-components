{
    "reporterEnabled": "mocha-junit-reporter, mochawesome",
    "mochaJunitReporterReporterOptions": {
        "mochaFile": "../../dist/cypress/apps/storybook-e2e/test-reports/junit-report-[hash].xml"
    },
    "mochawesomeReporterOptions": {
        "reportFilename": "[status]-[name]-report",
        "reportPageTitle": "Storybook e2e tests",
        "reportDir": "../../dist/cypress/apps/storybook-e2e/mochawesome-reports",
        "overwrite": false,
        "html": false,
        "json": true
    }
}
