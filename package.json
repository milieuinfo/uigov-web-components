{
    "name": "uigov-web-components",
    "version": "1.20.0",
    "license": "MIT",
    "scripts": {
        "test": "npm run test:unit && npm run test:wct",
        "test:component": "npx cypress run --component",
        "test:component-open": "npx cypress open --component",
        "test-parallel": "npm run test:unit:parallel && npm run test:wct",
        "test:unit": "npx nx run-many --all --target=test --skip-nx-cache",
        "test:unit:parallel": "npx nx run-many --all --target=test --parallel --maxParallel=4 --skip-nx-cache",
        "test:wct": "npm run build:all && npx nx wct map",
        "test:wctonly": "npx nx wct map",
        "test:wctdetail": "npm run build:all && npx nx wctdetail map",
        "test:wctdetailonly": "npx nx wctdetail map",
        "storybook": "npx nx storybook storybook http://localhost:8080",
        "storybook:test": "npx nx e2e storybook-e2e",
        "storybook:test-watch": "npx nx e2e storybook-e2e --watch",
        "storybook:build": "npx nx build-storybook storybook",
        "storybook:ci-test": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e'",
        "storybook:ci-test-parallel-1": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/components/[a-m]*/**/*'",
        "storybook:ci-test-parallel-2": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/components/[n-z]*/**/*'",
        "storybook:ci-test-parallel-3": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/elements/[a-i]*/**/*'",
        "storybook:ci-test-parallel-4": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/elements/[j-z]*/**/*'",
        "storybook:ci-test-parallel-5": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/sections/**/*'",
        "storybook:ci-test-parallel-6": "start-server-and-test 'npx nx storybook storybook' http://localhost:8080 'npx nx e2e storybook-e2e -- --spec **/src/e2e/map/**/*'",
        "playground-lit": "npx nx serve playground-lit",
        "playground-react": "npx nx serve playground-react",
        "playground-native": "npx nx serve playground-native",
        "playground-lit:test": "npx nx e2e playground-lit-e2e",
        "playground-react:test": "npx nx e2e playground-react-e2e",
        "playground-native:test": "npx nx e2e playground-native-e2e",
        "playground-lit:ci-test": "start-server-and-test 'npx nx serve playground-lit' http://localhost:4202 'npx nx e2e playground-lit-e2e'",
        "playground-react:ci-test": "start-server-and-test 'npx nx serve playground-react' http://localhost:4201 'npx nx e2e playground-react-e2e'",
        "playground-native:ci-test": "start-server-and-test 'npx nx serve playground-native' http://localhost:4203 'npx nx e2e playground-native-e2e'",
        "killstart:storybook": "npx kill-port 8080 && npx nx storybook storybook",
        "killstart:playground-legacy": "npx kill-port 4200 && npx nx serve playground-legacy",
        "killstart:playground-react": "npx kill-port 4201 && npx nx serve playground-react",
        "killstart:playground-lit": "npx kill-port 4202 && npx nx serve playground-lit",
        "killstart:playground-native": "npx kill-port 4203 && npx nx serve playground-native",
        "build:common-storybook": "npx nx build-with-js common-storybook",
        "build:common-utilities": "npx nx build-with-js common-utilities",
        "build:elements": "npx nx build-with-js elements",
        "build:components": "npx nx build-with-js components",
        "build:map": "npx nx build-with-js map",
        "build:qlik": "npx nx build-with-js qlik",
        "build:sections": "npx nx build-with-js sections",
        "build:test-support": "npx nx build-with-js support-test-support",
        "build:all": "npm run build:common-storybook && npm run build:common-utilities && npm run build:elements && npm run build:components && npm run build:map && npm run build:qlik && npm run build:sections && npm run build:test-support",
        "build-fat-lib": "npx nx build-fat-lib support-fat-lib",
        "pack:common-storybook": "cd ./dist/libs/common/storybook && npm version $npm_package_version && npm pack",
        "pack:common-utilities": "cd ./dist/libs/common/utilities && npm version $npm_package_version && npm pack",
        "pack:elements": "cd ./dist/libs/elements && npm version $npm_package_version && npm pack",
        "pack:components": "cd ./dist/libs/components && npm version $npm_package_version && npm pack",
        "pack:map": "cd ./dist/libs/map && npm version $npm_package_version && npm pack",
        "pack:qlik": "cd ./dist/libs/qlik && npm version $npm_package_version && npm pack",
        "pack:sections": "cd ./dist/libs/sections && npm version $npm_package_version && npm pack",
        "pack:test-support": "cd ./dist/libs/support/test-support && npm version $npm_package_version && npm pack",
        "pack:all": "npm run pack:common-storybook && npm run pack:common-utilities && npm run pack:elements && npm run pack:components && npm run pack:map && npm run pack:qlik && npm run pack:sections && npm run pack:test-support",
        "publish:common-storybook": "cd ./dist/libs/common/storybook && npm version $npm_package_version && npm publish",
        "publish:common-utilities": "cd ./dist/libs/common/utilities && npm version $npm_package_version && npm publish",
        "publish:elements": "cd ./dist/libs/elements && npm version $npm_package_version && npm publish",
        "publish:components": "cd ./dist/libs/components && npm version $npm_package_version && npm publish",
        "publish:map": "cd ./dist/libs/map && npm version $npm_package_version && npm publish",
        "publish:qlik": "cd ./dist/libs/qlik && npm version $npm_package_version && npm publish",
        "publish:sections": "cd ./dist/libs/sections && npm version $npm_package_version && npm publish",
        "publish:test-support": "cd ./dist/libs/support/test-support && npm version $npm_package_version && npm publish",
        "publish:all": "npm run publish:common-storybook && npm run publish:common-utilities && npm run publish:elements && npm run publish:components && npm run publish:map && npm run publish:qlik && npm run publish:sections && npm run publish:test-support"
    },
