{
    "name": "support-test-support",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/support/test-support/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "clean": true,
                "rootDir": "libs/support/test-support/src",
                "outputPath": "dist/libs/support/test-support",
                "main": "libs/support/test-support/index.ts",
                "tsConfig": "libs/support/test-support/tsconfig.lib.json",
                "assets": ["libs/support/test-support/*.md"],
                "updateBuildableProjectDepsInPackageJson": true
            }
        },
        "build-with-js": {
            "executor": "nx:run-commands",
            "dependsOn": ["build"],
            "options": {
                "command": "node tools/build-with-js/copy-test-support-js.mjs"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/support/test-support/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/support/test-support"],
            "options": {
                "jestConfig": "libs/support/test-support/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
