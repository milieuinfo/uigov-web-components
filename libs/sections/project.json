{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/sections/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "clean": true,
                "outputPath": "dist/libs/sections",
                "main": "libs/sections/src/index.ts",
                "tsConfig": "libs/sections/tsconfig.lib.json",
                "assets": ["libs/sections/*.md"]
            }
        },
        "build-fat-libs-min": {
            "executor": "@nrwl/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "options": {
                "clean": true,
                "outputPath": "dist/explicit-wrong-path",
                "main": "libs/sections/src/index.ts",
                "tsConfig": "libs/sections/fat-libs/tsconfig.fat-libs-min.json",
                "assets": [],
                "webpackConfig": "libs/sections/fat-libs/fat-libs-webpack-min.config.js",
                "isolatedConfig": true,
                "sourceMap": false
            }
        },
        "build-fat-libs": {
            "executor": "@nrwl/webpack:webpack",
            "dependsOn": ["build-fat-libs-min"],
            "outputs": ["{options.outputPath}"],
            "options": {
                "clean": false,
                "outputPath": "dist/explicit-wrong-path",
                "main": "libs/sections/src/index.ts",
                "tsConfig": "libs/sections/fat-libs/tsconfig.fat-libs.json",
                "assets": [],
                "webpackConfig": "libs/sections/fat-libs/fat-libs-webpack.config.js",
                "isolatedConfig": true,
                "sourceMap": true
            }
        },
        "build-with-js": {
            "executor": "nx:run-commands",
            "dependsOn": ["build"],
            "options": {
                "command": "node tools/build-with-js/copy-sections-js.mjs"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/sections/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/sections"],
            "options": {
                "jestConfig": "libs/sections/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": [],
    "name": "sections"
}
